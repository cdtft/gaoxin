<cdtftSql namespace="user">
    <select id="findOne" parameterType="com.github.cdtft.daotest.pojo.User" resultType="com.github.cdtft.daotest.pojo.User">
        select * from user where id = #{id}
    </select>
    <select id="findAll" parameterType="com.github.cdtft.daotest.pojo.User" resultType="com.github.cdtft.daotest.pojo.User">
        select * from user
    </select>
</cdtftSql>